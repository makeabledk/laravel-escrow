{
    "name": "makeabledk/laravel-escrow",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "Makeable\\LaravelEscrow\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.*",
        "makeabledk/php-value-objects": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "friendsofphp/php-cs-fixer": "^2.3",
        "laravel/laravel": "^5.4",
        "doctrine/dbal": "^2.5",
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^0.9.9",
        "laravel/cashier": "^7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Makeable\\LaravelEscrow\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "style": "./vendor/bin/php-cs-fixer fix ./src --rules=@PSR2,@Symfony,ordered_class_elements,ordered_imports --verbose"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
